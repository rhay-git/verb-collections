{
  "name": "verb-collections",
  "description": "Verb plugin that adds includes, layouts, badges and docs template collections. Can be used with verb, assemble or generate, in a `*file.js`, or a globally installed generator.",
  "version": "0.1.4",
  "homepage": "https://github.com/verbose/verb-collections",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "verbose/verb-collections",
  "bugs": {
    "url": "https://github.com/verbose/verb-collections/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "generate-collections": "^0.3.7"
  },
  "devDependencies": {
    "engine-base": "^0.1.2",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-format-md": "^0.1.11",
    "gulp-istanbul": "^1.1.1",
    "gulp-mocha": "^3.0.1",
    "mocha": "^3.2.0",
    "verb": "github:verbose/verb#dev"
  },
  "keywords": [
    "collections",
    "docs",
    "documentation",
    "plugin",
    "verb",
    "verbplugin"
  ],
  "verb": {
    "run": true,
    "toc": "collapsible",
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "assemble",
        "generate",
        "verb",
        "verb-readme-generator"
      ]
    },
    "reflinks": [
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
